/* components/myTable/myTable.wxss */
.tableTou {
  /* flex: 1; */
  height: 72rpx;
  background: #F7F7F7;
  border: 2rpx solid #EEEEEE;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  
  color: #333333;
}

.tableTR {
  /* flex: 1; */
  height: 72rpx;
  background: #FFFFFF;
  border: 2rpx solid #EEEEEE;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  
  color: #333333;


  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.sticky-top {
  position: sticky;
  top: 0;
  left: 0;
  z-index: 99;
}

.sticky-bottom {
  position: sticky;
  left: 0;
  z-index: 99;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

/* 可以点击前往上课记录 */
.toDetail {
  color: #14B900;
}